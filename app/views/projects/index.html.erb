<% if notice.present? %>
  <p id="notice"><%= notice %></p>
<% end %>
<br />
<br />
<div class="row project-text">
  <h1 class="text-center">Projects</h1>
</div>
  <% @projects.each do |project| %>
    <div class="row project-text">
      <div class="col-sm-3">
        <h2><%= project.title %></h2>
      </div>
      <div class="col-sm-3">
        <%= project.versions %>
      </div>
      <div class="col-sm-3">
        <%= image_tag project.project_image, class: 'project-index-image' %>
      </div>
      <div class="col-sm-3">
        <%= link_to 'details'.upcase, project, class: 'more' %>
      </div>
    </div>
  <% end %>
  <br />
  <br />
  <br />
  <br />
  <br />
  <style>
  body {
  overflow-x: hidden !important;
  }
  </style>
  
